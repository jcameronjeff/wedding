 {{>_navbar}} {{>_logo}}


<header id="collapse">

  <div class="view jarallax" data-jarallax='{"speed": 0.2}' style="background-image: url('img/DSC_4235.jpg'); background-repeat: no-repeat; background-size: cover; background-position: center center;">


    <!-- Mask & flexbox options-->
    <div class="mask rgba-dark-strong d-flex justify-content-center align-items-center">
      <!-- Content -->
      <div class="container">

        <!--Grid row-->
        <div class="row">

          <!--Grid column-->
          <div class="col-md-12 white-text text-center">
            <h1 class="display-3 pt-md-5 white-text font-weight-bold wow fadeInDown" style="font-family: 'Great Vibes', cursive; letter-spacing: 10px;"
              data-wow-delay="0.3s">Ashley + Jeff
            </h1>
          </div>
          <!--Grid column-->
        </div>
        <!--Grid row-->
        <div class="row mt-3">
          <div class="col-md-12">
            <h2 class="h2-responsive white-text text-center wow fadeInDown" style="font-family: 'Great Vibes', cursive;" data-wow-delay="0.3s">

              14 October 2018 - Savannah, Georgia</h2>
          </div>
        </div>

        <div class="row mt-3">
          {{>_countdown}}

        </div>
      </div>
      <!-- Content -->
    </div>
    <!-- Mask & flexbox options-->
  </div>
</header>
