<header id="collapse">

    <div class="view jarallax" data-jarallax='{"speed": 0.2}' style="background-image: url('img/002.jpg'); background-repeat: no-repeat; background-size: contain; background-position: center;">


        <!-- Mask & flexbox options-->
        <div class="mask rgba-black-strong d-flex justify-content-center align-items-center">
            <!-- Content -->
            <div class="container">

                <!--Grid row-->
                <div class="row">

                    <!--Grid column-->
                    <div class="col-md-12 white-text text-center">
                        <h1 class="display-3 pt-md-5 white-text font-weight-bold wow fadeInDown" style="font-family: 'Great Vibes', cursive; letter-spacing: 10px;"
                            data-wow-delay="0.3s">Events
                        </h1>
                    </div>
                    <!--Grid column-->
                </div>
                <!--Grid row-->


            </div>
            <!-- Content -->
        </div>
        <!-- Mask & flexbox options-->
    </div>
</header>
<!-- Grid row -->

<div id="collapse2" class="row pt-5 d-flex justify-content-center">

    <!-- Grid column -->
    <div class="col-md-2 pl-2 pl-md-0 pb-5">

        <!-- Stepper -->
        <div class="steps-form-3">
            <div class="steps-row-3 setup-panel-3 d-flex justify-content-between">
                <div class="steps-step-3">
                    <a href="#step-5" type="button" class="btn btn-info btn-circle-3 waves-effect ml-0" data-toggle="tooltip" data-placement="top"
                        title="Welcome Party">
                        <i class="fa fa-glass" aria-hidden="true"></i>
                    </a>
                </div>
                <div class="steps-step-3">
                    <a href="#step-6" type="button" class="btn btn-pink btn-circle-3 waves-effect p-3" data-toggle="tooltip" data-placement="top"
                        title="Ceremony">
                        <i class="fa fa-heart" aria-hidden="true"></i>
                    </a>
                </div>
                <div class="steps-step-3">
                    <a href="#step-7" type="button" class="btn btn-pink btn-circle-3 waves-effect" data-toggle="tooltip" data-placement="top"
                        title="Reception">
                        <i class="fa fa-birthday-cake" aria-hidden="true"></i>
                    </a>
                </div>
                <div class="steps-step-3 no-height">
                    <a href="#step-8" type="button" class="btn btn-pink btn-circle-3 waves-effect p-3" data-toggle="tooltip" data-placement="top"
                        title="Departures">
                        <i class="fa fa-plane" aria-hidden="true">
                        </i>

                    </a>
                </div>
            </div>
        </div>

    </div>
    <!-- Grid column -->

    <!-- Grid column -->
    <div class="col-md-7">


        <!-- First Step -->
        <div class="row setup-content-3" id="step-5">
            <div class="col-md-12">
                <h3 class="font-weight-bold pl-0 my-4">
                    <strong>Welcome Party</strong>
                </h3>

                <button class="btn btn-mdb-color btn-rounded nextBtn-3 float-right" type="button">Next</button>
            </div>
        </div>


        <!-- Second Step -->
        <div class="row setup-content-3" id="step-6">
            <div class="col-md-12">
                <h3 class="font-weight-bold pl-0 my-4">
                    <strong>Ceremony</strong>
                </h3>

                <button class="btn btn-mdb-color btn-rounded prevBtn-3 float-left" type="button">Previous</button>
                <button class="btn btn-mdb-color btn-rounded nextBtn-3 float-right" type="button">Next</button>
            </div>
        </div>


        <!-- Third Step -->
        <div class="row setup-content-3" id="step-7">
            <div class="col-md-12">
                <h3 class="font-weight-bold pl-0 my-4">
                    <strong>Reception</strong>
                </h3>

                <button class="btn btn-mdb-color btn-rounded prevBtn-3 float-left" type="button">Previous</button>
                <button class="btn btn-mdb-color btn-rounded nextBtn-3 float-right" type="button">Next</button>
            </div>
        </div>


        <!-- Fourth Step -->
        <div class="row setup-content-3" id="step-8">
            <div class="col-md-12">
                <h3 class="font-weight-bold pl-0 my-4">
                    <strong>Departures</strong>
                </h3>

                <button class="btn btn-mdb-color btn-rounded prevBtn-3 float-left" type="button">Previous</button>
                <button class="btn btn-success btn-rounded float-right" type="submit">Submit</button>
            </div>
        </div>


    </div>
    <!-- Grid column -->

</div>
<!-- Grid row -->